{
    "manifest_version": 2,
    "name": "Google在线状态",
    "version": "1.0",
    "description": "监视Google是否在线",
   
    "browser_action": {
        "default_icon": {
            "19": "images/cat.png"
        },
        "default_popup": "index.html"
    },
    "background": {
        "scripts": [
            "js/status.js"
        ]
       
    },
    "permissions": [
        "https://www.google.cn/"
    ]
}